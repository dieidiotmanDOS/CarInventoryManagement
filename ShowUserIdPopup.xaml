<Window x:Class="CarInventoryManagement.ShowUserIdPopup"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CarInventoryManagement"
        mc:Ignorable="d"
        Title="Setup" Height="150" Width="250"
        ResizeMode="NoResize">

    <Grid>
        <TextBlock x:Name="useridTextBlock" HorizontalAlignment="Center" Margin="0,28,0,0" TextAlignment="Center" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Height="34" Width="180"/>
        <Button x:Name="CloseButton" Content="Close" HorizontalAlignment="Center" Margin="0,67,0,0" VerticalAlignment="Top" Click="CloseButton_Click" Width="58"/>

    </Grid>
</Window>
